{
    "breadcrumbs.enabled": true,
    "diffEditor.ignoreTrimWhitespace": false,
    // https://qiita.com/yoskeoka/items/3b18ed4a40267aacc76f
    "editor.copyWithSyntaxHighlighting": false,
    // "editor.fontFamily": "SourceHanCodeJP-Regular",
    // "editor.fontSize": 11,
    "editor.formatOnPaste": true,
    "editor.formatOnSave": false,
    "editor.minimap.enabled": false,
    "editor.minimap.renderCharacters": false,
    "editor.minimap.showSlider": "always",
    "editor.renderControlCharacters": true,
    "editor.renderLineHighlight": "all",
    "editor.renderWhitespace": "all",
    "editor.rulers": [
        80,
        100
    ],
    "editor.snippetSuggestions": "top",
    "explorer.autoReveal": false,
    "extensions.ignoreRecommendations": false,
    "files.associations": {
        "javascript.json": "jsonc"
    },
    // "files.exclude": {
    //     "**/.git": true,
    //     "**/.svn": true,
    //     "**/.hg": true,
    //     "**/CVS": true,
    //     "**/.DS_Store": true,
    //     "**/__pycache__": true,
    //     "**/.ropeproject": true,
    //     "**/.mypy_cache": true,
    // },
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    // "git.enableSmartCommit": true,
    // https://github.com/powerline/fonts
    // "terminal.integrated.fontFamily": "'Source Code Pro for Powerline'",
    "terminal.integrated.scrollback": 20000,
    "window.title": "${dirty}${activeEditorMedium}${separator}${rootName}",
    "workbench.colorCustomizations": {
        "[Default Dark+]": {
            "editor.lineHighlightBackground": "#333333",
        },
    },
    // Language specific editor settings
    "[javascript]": {
        "editor.formatOnPaste": false,
        "editor.formatOnSave": false,
        "editor.tabSize": 2
    },
    "[json]": {
        "editor.formatOnPaste": false,
        "editor.formatOnSave": false,
        "editor.tabSize": 2,
    },
    "[python]": {
        "editor.formatOnPaste": false,
        "editor.formatOnSave": false,
        "editor.tabSize": 4
    },
    "[shellscript]": {
        "editor.formatOnPaste": false,
        "editor.formatOnSave": false,
        "editor.tabSize": 2
    },
    "[tsv]": {
        "files.insertFinalNewline": false,
        "files.trimTrailingWhitespace": false,
    },
    // Extension settiong
    // "bashBeautify.tabSize": 2,
    // "bracketPairColorizer.showHorizontalScopeLine": false,
    // "csv-preview.capitalizeHeaders": false,
    // "csv-preview.lineNumbers": "ordinal",
    // "csv-preview.theme": "light",
    // "eslint.autoFixOnSave": true,
    // "eslint.validate": [
    //     "javascript",
    //     "javascriptreact",
    //     {
    //         "language": "vue",
    //         "autoFix": true
    //     }
    // ],
    "gitlens.codeLens.authors.enabled": false,
    "gitlens.codeLens.recentChange.enabled": false,
    "liveServer.settings.CustomBrowser": "chrome",
    "liveServer.settings.donotShowInfoMsg": true,
    // "python.autoComplete.extraPaths": [
    //     "~/.pyenv/versions/3.7.0/lib/python3.7/site-packages"
    // ],
    // "python.formatting.autopep8Args": [
    //     "--ignore=E501"
    // ],
    // "python.linting.enabled": false,
    // "python.linting.pylintEnabled": false,
    // "python.linting.flake8Enabled": true,
    // "python.linting.flake8Args": [
    //     "--max-line-length=120",
    //     "--ignore=E402"
    // ],
    // "python.pythonPath": "~/.pyenv/shims/python",
    "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
    "todo-tree.general.tags": [
        "BUG",
        "FIXME", // 既知の不具合があるコード。修正が必要。
        "HACK",  // あまりきれいじゃないコード。リファクタリングが必要。
        "NOTE",  // なぜ、こうなったという情報を残す。
        "TODO",  // あとで追加、修正するべき機能がある。
        "XXX",   // 危険！動くけどなぜうごくかわからない。
        "[ ]",
        "[x]",
    ],
    "todo-tree.highlights.customHighlight": {
        "BUG": {
            "foreground": "#E74C3C",
            "iconColour": "#E74C3C",
        },
        "FIXME": {
            "background": "#FF69B4",
            "foreground": "#FFFFFF",
            "iconColour": "#FF69B4",
            "opacity": 80,
        },
        "HACK": {
            "background": "#FF8C00",
            "foreground": "#FFFFFF",
            "iconColour": "#FF8C00",
            "opacity": 80,
        },
        "NOTE": {
            "foreground": "#00FFFF",
            "icon": "comment",
            "iconColour": "#00FFFF",
        },
        "TODO": {},
        "XXX": {
            "background": "#E74C3C",
            "foreground": "#FFFFFF",
            "icon": "alert",
            "iconColour": "#E74C3C",
            "opacity": 80,
        },
        "[ ]": {},
        "[x]": {
            "foreground": "#808080",
            "iconColour": "#808080",
            "type": "text",
        },
    },
    "todo-tree.highlights.defaultHighlight": {
        "foreground": "#FFFF00",
        "iconColour": "#FFFF00",
        "type": "tag",
    },
    "javascript.updateImportsOnFileMove.enabled": "always",
    "sync.gist": "eb25a4c56c0e13bd1ec41b662a7c3f6b",
    "vsnotes.defaultNotePath": "C:/Users/taken/Desktop/VSNotes",
    "vsnotes.templates": [
        "today",
    ]
    // "open-file-from-path.startingPath": "/,tests/",
    // "path-intellisense.mappings": {
    //     "tests": "${workspaceRoot}/tests",
    // },
    // "rest-client.decodeEscapedUnicodeCharacters": true,
    // "rest-client.previewOption": "exchange",
