{
	// Place your snippets for python here. Each snippet is defined under a snippet name and has a prefix, body and
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	"class": {
		"prefix": "class",
		"body": [
			"class ${1:ClassName} {",
			"    constructor($2) {",
			"        ${3:console.log(\"constructor\");}",
			"    }",
			"",
			"    ${4:method}(${5}) {",
			"        ${6:console.log(\"method\");}",
			"    }",
			"}",
			""
		],
		"description": "Code snippet for defining a class."
	},
	"try/catch/finally": {
		"prefix": "try/catch/finally",
		"body": [
			"try {",
			"    ${2}",
			"} catch (${1:error}) {",
			"    ",
			"} finally {",
			"    ",
			"}",
			""
		],
		"description": "Code snippet for try/catch/finally statement."
	},
	"promise": {
		"prefix": "promise",
		"body": [
			"new Promise((${1:resolve}, ${2:reject}) => {",
			"    ${3:console.log(\"do something\");}",
			"})",
			""
		],
		"description": "Code snippet for an new promise"
	}
}
